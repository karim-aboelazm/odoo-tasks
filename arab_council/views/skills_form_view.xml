<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="skills_view_form" model="ir.ui.view">
            <field name="name">arab_council.skills.form</field>
            <field name="model">arab_council.skills</field>
            <field name="arch" type="xml">
                <form>
                    <sheet string="arab_council.skills">
                        <group col="4">
                            <newline/>
                            <field name="skills" colspan="4"/>
                            <newline/>
                            <field name="cv"/>
                            <separator/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>